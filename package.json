{
  "name": "house-of-panda",
  "private": true,
  "engines": {
    "node": "^18.16.0"
  },
  "scripts": {
    "lint": "next lint",
    "dev:wss": "nodemon",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "generate": "next build && next export",
    "start": "link-module-alias && node dist/src/server/index.js",
    "build": "NODE_ENV=production run-s build:*",
    "build:next": "RUN_MODE=build NODE_ENV=production next build",
    "build:server": "tsc --project tsconfig.server.json",
    "abi:sync": "cp ../hop-contract/build/contracts/HouseOfPanda.json ../hop-contract/build/contracts/Staker.json ./src/lib/contracts/",
    "abi:generate": "npx abi-types-generator './src/lib/contracts/HouseOfPanda.json' --output='./src/lib/types' --name=HouseOfPandaWrapper && npx abi-types-generator './src/lib/contracts/Staker.json' --output='./src/lib/types' --name=StakerWrapper",
    "db:seed": "ts-node --project tsconfig.seeder.json ./seed",
    "db:sync-project": "ts-node --project tsconfig.seeder.json ./seed/syncNewContract.ts",
    "deploy:dev": "sh -e './etc/script/deploy-web.sh'",
    "deploy:prod": "sh -e './etc/script/deploy-prod.sh'",
    "cryptutil:encrypt": "node ./utils/util.js --encrypt",
    "cryptutil:decrypt": "node ./utils/util.js --decrypt",
    "cryptutil:validate": "node ./utils/util.js --validate",
    "export:indonesia": "mongoexport --collection=indonesias --db=hop --out=./seed/data/indonesias.json --jsonArray",
    "start-listener": "ts-node --project tsconfig.seeder.json ./event_listener"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.9.0",
    "@tanstack/react-query": "^4.33.0",
    "@tinymce/tinymce-react": "^4.2.0",
    "@trpc/client": "^10.38.0",
    "@trpc/next": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "@web3-react/walletlink-connector": "^6.2.14",
    "bcrypt": "^5.1.0",
    "browser-image-compression": "^2.0.0",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ethereumjs-util": "^7.1.5",
    "express-jwt": "^6.1.1",
    "framer-motion": "7.5.0",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "6.5.1",
    "next": "^12.3.4",
    "next-seo": "^5.15.0",
    "nextjs-google-analytics": "^2.3.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2.0",
    "react-animated-css": "^1.2.1",
    "react-calendar": "^4.0.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.39.2",
    "react-intersection-observer": "^9.4.1",
    "react-lottie": "^1.2.3",
    "react-multi-carousel": "^2.8.2",
    "react-redux": "^8.0.5",
    "react-responsive-combo-box": "^1.3.0",
    "react-social-icons": "^5.15.0",
    "react-time-picker": "^6.5.0",
    "react-tooltip": "^5.10.4",
    "remark": "^14.0.3",
    "remark-custom-heading-id": "^1.0.1",
    "remark-html": "^15.0.2",
    "rxjs": "^7.5.5",
    "sass": "^1.49.7",
    "styled-components": "^5.3.3",
    "superjson": "^1.13.1",
    "unified": "^10.1.2",
    "web3": "^1.8.0",
    "web3-eth-contract": "^1.8.0",
    "web3.storage": "^4.4.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-calendar": "^3.9.0",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "^18.0.10",
